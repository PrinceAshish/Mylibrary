<h2>Search Authors</h2>
<form action="/Authors" method="get">
<label for="nameId">Name :</label>
<input type="text" name="name" id="nameId" value="<%= boxName.name %> ">
<button type="submit">Search</button>
</form>
<br>
<% for(let Author of Authors){ %>
    <div><%= Author.name %> </div> 
    <%  } %> 